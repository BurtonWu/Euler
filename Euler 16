package euler;
import java.math.BigDecimal;

public class Euler16 {

	   public void run()
	   {
	      int sum = 0;
	      BigDecimal bd = new BigDecimal(Math.pow(2, 1000));
	      String str = bd.toString();
	      for(int i = 0; i < str.length(); i++)
	      {
	         sum += (int)(str.charAt(i) - '0');			
	      }
	      System.out.println(sum);
	   }
}
