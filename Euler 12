package euler;

public class Euler12 {

	public void run()
	{
		int count = 0;
	    int incrementor = 1;
	    long num = 1L;
	    while(count < 501)
	    {
	       count = 0;
	       incrementor++;
	       num += incrementor;
	       if(num%2 == 0 && num%3 == 0 && num %4 == 0 && num%5 == 0)
         {
	    	   for(int i = 1; i <= num; i++)
	         {
	    		   if(num%i == 0)
	                  count++;
	         }
	       }
	    }
	      System.out.println(num);
	  }
}
