package euler;

public class Euler28 
{
	public void run()
	{
		int temp, index = 9;
		int retval = 1+3+5+7+9;
		for(int sideMat = 5; sideMat <= 1001; sideMat+=2)
		{
			temp = index;
			for(int side = 1; side <= 4; side++)
			{
				index += sideMat-1;
				retval += index;
				System.out.println(index);
			}
		}
		System.out.println(retval);
	}
}
